		"@types/node": "^16.7.13",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.22",
		"@types/redux-saga": "^0.10.5",
		"axios": "^0.25.0",
		"babel-polyfill": "^6.26.0",
		"crypto-js": "^4.1.1",
		"i18next": "^21.6.7",
		"immer": "^9.0.12",
		"produce": "^0.4.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-i18next": "^11.15.3",
		"react-redux": "^7.2.6",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
